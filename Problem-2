#include <stdio.h>

int main() {
    int n,i,c=1;
    scanf("%d",&n);
    if (n%2==0)
    {
               for(i=1;i<=n-1;i++)
        {
            printf("%d ",c);
            c=c+2;
        }
    }
    else
    {
       for(i=1;i<=n;i++)
        {
            printf("%d ",c);
            c=c+2;
        }
      
      
    }
    return 0;
}
